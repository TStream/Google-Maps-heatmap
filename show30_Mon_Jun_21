  customIcons["dru"] = dru;
  customIcons["dui"] = dui;
  customIcons["fra"] = fra;
  customIcons["hom"] = hom;
  customIcons["rob"] = rob;
  customIcons["rap"] = rap;
  customIcons["sex"] = sex;
  customIcons["sus"] = sus;
  customIcons["the"] = the;
  customIcons["van"] = van;
  customIcons["veh"] = veh;
  customIcons["wea"] = wea;

  var markerGroups = { "ars": [], "ass": [], "bur": [], "dis": [], "dru": [], "dui": [], "fra": [], "hom": [], "rob": [], "rap": [], "sex": [], "sus": [], "the": [], "van": [], "veh": [], "wea": [] };

  function createMarker(point, icon, html) {
        var marker = new GMarker(point, {icon: customIcons[icon], hide: true});
        markerGroups[icon].push(marker);
    GEvent.addListener(marker, "click", function() {
      marker.openInfoWindowHtml(html);
    });
    return marker;
  }

