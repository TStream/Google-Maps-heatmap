  function toggleGroup(type) {
    for (var i = 0; i < markerGroups[type].length; i++) {
      var marker = markerGroups[type][i];
      if (marker.isHidden()) {
        marker.show();
      } else {
        marker.hide();
      }
    } 
  }

  function hideGroup(type) {
    for (var i = 0; i < markerGroups[type].length; i++) {
      var marker = markerGroups[type][i];
      marker.hide();
    } 
  }

  function showGroup(type) {
    for (var i = 0; i < markerGroups[type].length; i++) {
      var marker = markerGroups[type][i];
      marker.show();
    } 
  }

  function checkAll() {
    arsCheckbox.checked = true ;
    assCheckbox.checked = true ;
    burCheckbox.checked = true ;
    disCheckbox.checked = true ;
    druCheckbox.checked = true ;
    duiCheckbox.checked = true ;
    fraCheckbox.checked = true ;
    homCheckbox.checked = true ;
    robCheckbox.checked = true ;
    rapCheckbox.checked = true ;
    sexCheckbox.checked = true ;
    susCheckbox.checked = true ;
    theCheckbox.checked = true ;
    vanCheckbox.checked = true ;
    vehCheckbox.checked = true ;
    weaCheckbox.checked = true ;
    showGroup('ars');
    showGroup('ass');
    showGroup('bur');
    showGroup('dis');
    showGroup('dru');
    showGroup('dui');
    showGroup('fra');
    showGroup('hom');
    showGroup('rob');
    showGroup('rap');
    showGroup('sex');
    showGroup('sus');
    showGroup('the');
    showGroup('van');
    showGroup('veh');
    showGroup('wea');
  }

  function uncheckAll() {
    arsCheckbox.checked = false ;
    assCheckbox.checked = false ;
    burCheckbox.checked = false ;
    disCheckbox.checked = false ;
    druCheckbox.checked = false ;
    duiCheckbox.checked = false ;
    fraCheckbox.checked = false ;
    homCheckbox.checked = false ;
    robCheckbox.checked = false ;
    rapCheckbox.checked = false ;
    sexCheckbox.checked = false ;
    susCheckbox.checked = false ;
    theCheckbox.checked = false ;
    vanCheckbox.checked = false ;
    vehCheckbox.checked = false ;
    weaCheckbox.checked = false ;
    hideGroup('ars');
    hideGroup('ass');
    hideGroup('bur');
    hideGroup('dis');
    hideGroup('dru');
    hideGroup('dui');
    hideGroup('fra');
    hideGroup('hom');
    hideGroup('rob');
    hideGroup('rap');
    hideGroup('sex');
    hideGroup('sus');
    hideGroup('the');
    hideGroup('van');
    hideGroup('veh');
    hideGroup('wea');
  }

</script>

